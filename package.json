{
  "name": "ym-userequest",
  "description": "基于Vue3的 useRequest",
  "version": "1.0.4",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": {
    "name": "zhushengjie"
  },
  "scripts": {
    "build": "tsup",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"lib/**/*.ts\" \"lib/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YeMao-Zi/ym-useRequest"
  },
  "keywords": [
    "useRequest",
    "vue3",
    "hooks"
  ],
  "devDependencies": {
    "tsup": "^6.7.0",
    "@types/jest": "^29.5.3",
    "jest": "^29.6.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6",
    "vue": "^3.3.4"
  }
}
