{
  "name": "ym-userequest",
  "description": "useRequest for vue3",
  "version": "1.14.1-beta.0",
  "private": false,
  "exports": {
    ".": {
      "require": {
        "node": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "node": "./dist/index.mjs",
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./*": "./*",
    "./dist/*": "./dist/*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": {
    "name": "yemao-zi"
  },
  "scripts": {
    "dev": "vite --config ./playground/vite.config.ts ./playground",
    "build": "tsup",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "publishing": "release-it",
    "release": "tsup && release-it",
    "release:alpha": "tsup && release-it --preRelease=alpha",
    "release:beta": "tsup && release-it --preRelease=beta",
    "release:dry-run": "tsup && release-it --dry-run",
    "release:alpha:dry-run": "tsup && release-it --preRelease=alpha --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YeMao-Zi/ym-useRequest"
  },
  "keywords": [
    "request",
    "useRequest",
    "hooks"
  ],
  "devDependencies": {
    "@release-it/conventional-changelog": "^9.0.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitest/coverage-v8": "^0.34.3",
    "jsdom": "^22.1.0",
    "release-it": "^17.10.0",
    "tsup": "^8.0.2",
    "typescript": "^5.0.0",
    "vite": "^4.2.0",
    "vitest": "^0.32.0",
    "vue": "^3.2.47",
    "@swc/core": "^1.4.12"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
