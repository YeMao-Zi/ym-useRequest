{
  "name": "ym-userequest",
  "description": "基于Vue3的 useRequest",
  "version": "1.3.1",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.mjs",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "index.mjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": {
    "name": "zhushengjie"
  },
  "scripts": {
    "dev": "vite --config ./playground/vite.config.ts ./playground",
    "build": "tsup",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "publishing": "release-it",
    "release": "yarn build && yarn publishing",
    "format": "prettier --write \"lib/**/*.ts\" \"lib/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YeMao-Zi/ym-useRequest"
  },
  "keywords": [
    "useRequest",
    "vue3",
    "hooks"
  ],
  "devDependencies": {
    "@release-it/conventional-changelog": "^7.0.2",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitest/coverage-v8": "^0.34.3",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "release-it": "^16.2.1",
    "tsup": "^6.7.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.3",
    "vue": "^3.3.4"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
